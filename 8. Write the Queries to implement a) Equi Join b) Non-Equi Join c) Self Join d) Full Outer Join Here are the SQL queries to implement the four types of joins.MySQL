/a) Equi_Join:
SELECT *
FROM table1
JOIN table2
ON table1.column_name = table2.column_name;

/b) Non-Equi_Join
SELECT *
FROM table1
JOIN table2
ON table1.column_name > table2.column_name;

/c) Self_Join
SELECT *
FROM table1 t1
JOIN table1 t2
ON t1.column_name = t2.column_name;

/d) Full Outer_Join:
SELECT *
FROM table1
FULL OUTER JOIN table2
ON table1.column_name = table2.column_name;
